{
  "name": "reactor/reactorcms",
  "description": "Polymorphic and multilingual CMS powered by Laravel 5.",
  "keywords": [
    "kenarkose",
    "nuclear",
    "nuclearcms",
    "hierarchy",
    "polymorphic",
    "multilingual",
    "reactor",
    "cms",
    "content",
    "management",
    "laravel",
    "laravel5"
  ],
  "version": "3.0-alpha.15",
  "authors": [
    {
      "name": "Hyleeh",
      "email": "egemen@kenarkose.com",
      "homepage": "http://egemenkizilcan.com",
      "role": "Developer / Designer"
    }
  ],
  "license": "MIT",
  "type": "project",

  "require": {
    "php": ">=7.0",
    "barryvdh/laravel-debugbar": "~2.0",
    "chumper/zipper": "^1.0",
    "fideloper/proxy": "^4.0",
    "igaster/laravel-theme": "^2.0",
    "laracasts/flash": "^2.0",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "^1.0",
    "propaganistas/laravel-cache-keywords": "~1.0",
    "reactor/documents": "dev-master",
    "reactor/hierarchy": "dev-master",
    "reactor/users": "dev-master",
    "spatie/laravel-cors": "^1.3",
    "tymon/jwt-auth": "^1.0.0-rc.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "ReactorCMS\\": "app/",
      "gen\\": "gen/",
      "Extension\\": "extension/"
    }
  },
  "autoload-dev": {
    "classmap": [

    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
